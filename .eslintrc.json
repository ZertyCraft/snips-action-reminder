{
    "env": {
        "es6": true,
        "node": true,
        "jest/globals": true
    },
    "extends": "eslint:recommended",
    "plugins": ["jest"],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console": "off",
        "quotes": ["error", "single", { "avoidEscape": true }],
        "quote-props": ["error", "as-needed"]
    }
}